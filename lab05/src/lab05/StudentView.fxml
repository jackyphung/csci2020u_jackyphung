<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane fx:controller="lab05.Controller"
            xmlns:fx="http://javafx.com/fxml">
    <TableView fx:id="tabView" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0">
        <columns>
            <TableColumn text="SID">
                <cellValueFactory>
                    <PropertyValueFactory property="studentID"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Assignments">
                <cellValueFactory>
                    <PropertyValueFactory property="midterm"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Midterm">
                <cellValueFactory>
                    <PropertyValueFactory property="assignment"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Final Exam">
                <cellValueFactory>
                    <PropertyValueFactory property="finalExam"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Final Mark">
                <cellValueFactory>
                    <PropertyValueFactory property="finalMark"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Letter Grade">
                <cellValueFactory>
                    <PropertyValueFactory property="letterGrade"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</AnchorPane>